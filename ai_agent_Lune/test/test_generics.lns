class Box<T> {
    let val: T;

    pub fn __init( val: T ) {
        self.val = val;
    }

    pub fn get(): T {
        return self.val;
    }
}

// int 型の Box
let iBox = new Box<int>( 10 );
print( iBox.get() + 1 );

// Constraint test
